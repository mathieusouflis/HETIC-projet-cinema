{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Application",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2022"],
    "target": "ES2022",
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "sourceMap": false,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.cache/tsbuildinfo.json"
  },
  "exclude": ["node_modules", "dist", "build", "coverage"]
}
