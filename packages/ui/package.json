{
  "name": "@packages/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist/**",
    "dist"
  ],
  "exports": {
    "./counter-button": {
      "import": {
        "types": "./dist/es/counter-button.d.mts",
        "default": "./dist/es/counter-button.mjs"
      },
      "require": {
        "types": "./dist/cjs/counter-button.d.ts",
        "default": "./dist/cjs/counter-button.js"
      }
    },
    "./link": {
      "import": {
        "types": "./dist/es/link.d.mts",
        "default": "./dist/es/link.mjs"
      },
      "require": {
        "types": "./dist/cjs/link.d.ts",
        "default": "./dist/cjs/link.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "check-types": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run"
  },
  "devDependencies": {
    "@packages/eslint-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@packages/vitest-presets": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.15.3",
    "@vitest/coverage-v8": "^4.0.15",
    "bunchee": "^6.4.0",
    "eslint": "^9.39.0",
    "jsdom": "^25.0.1",
    "typescript": "5.9.3",
    "vite": "^7.2.0",
    "vitest": "4.0.15"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.15"
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  }
}
