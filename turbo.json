{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "generate-api-sdk": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^generate-api-sdk"]
    },
    "generate-websocket-models": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^generate-websocket-models"]
    },
    "generate-websocket-sdk": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^generate-websocket-sdk"]
    },
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "build:watch": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "check-types": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^build"],
      "env": ["FRONTEND_PORT", "FRONTEND_HOST", "FRONTEND_URL", "VITE_PUBLIC_POSTHOG_KEY", "VITE_PUBLIC_POSTHOG_HOST", "NODE_ENV" ],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": ["^build"]
    }
  }
}
